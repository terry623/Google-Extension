{
  "manifest_version": 2,
  "name": "NTHU Course",
  "description": "幫助你的選課",
  "version": "2.0",
  "browser_action": {
    "default_title": "我要選到課!",
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["jquery-3.3.1.min.js", "content.js"],
      "css": ["Flat-UI/dist/css/flat-ui.css"]
    }
  ],
  "permissions": ["activeTab"]
}
