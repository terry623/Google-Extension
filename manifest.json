{
  "manifest_version": 2,
  "name": "NTHU Course",
  "description": "幫助你的選課",
  "version": "2.0",
  "browser_action": {
    "default_title": "我要選到課!",
    "default_icon": "./static/icon.png"
  },
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["./dist/bundle.js"]
    }
  ]
}
